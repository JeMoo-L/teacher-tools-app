{
  "name": "teacher-tools-app",
  "productName": "teacher-tools-app",
  "version": "1.0.8",
  "description": "My Electron application description",
  "main": ".vite/main/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "dev": "NODE_ENV=development electron .",
    "release": "electron-builder build -mwl --publish always",
    "build": "electron-builder build --publish always",
    "build:all": "electron-builder build -mwl",
    "build:win": "cross-env NODE_ENV=production electron-builder --win",
    "build:mac": "cross-env NODE_ENV=production electron-builder --mac",
    "build:linux": "cross-env NODE_ENV=production electron-builder --linux"
  },
  "keywords": [],
  "author": "wll",
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.7.0",
    "@electron-forge/maker-deb": "^7.7.0",
    "@electron-forge/maker-rpm": "^7.7.0",
    "@electron-forge/maker-squirrel": "^7.7.0",
    "@electron-forge/maker-zip": "^7.7.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.7.0",
    "@electron-forge/plugin-fuses": "^7.7.0",
    "@electron-forge/plugin-vite": "^7.7.0",
    "@electron/fuses": "^1.8.0",
    "@tailwindcss/postcss": "^4.0.9",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/store": "^2.0.5",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "electron": "34.3.0",
    "electron-builder": "^25.1.8",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.31.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.0.9",
    "ts-node": "^10.9.2",
    "typescript": "~4.5.4",
    "vite": "^5.4.14"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.6",
    "@mui/material": "^6.4.5",
    "@sentry/react": "^9.2.0",
    "ahooks": "^3.8.4",
    "electron-log": "^5.3.0",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.3.9",
    "jotai": "^2.12.1",
    "jotai-optics": "^0.4.0",
    "lucide-react": "^0.476.0",
    "optics-ts": "^2.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animated": "^2.0.0",
    "uuid": "^11.1.0"
  },
  "build": {
    "appId": "com.github.JeMoo-L.teacher-tools-app",
    "publish": {
      "provider": "github",
      "owner": "JeMoo-L",
      "repo": "teacher-tools-app"
    },
    "mac": {
      "category": "public.app-category.productivity",
      "target": [
        "dmg",
        "zip"
      ],
      "hardenedRuntime": true,
      "gatekeeperAssess": false
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ],
      "certificateFile": "cert.pfx",
      "certificatePassword": ""
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Development"
    }
  }
}
